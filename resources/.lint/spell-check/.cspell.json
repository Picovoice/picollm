{
  "language": "en",
  "dictionaries": [
    "dict"
  ],
  "dictionaryDefinitions": [
    {
      "name": "dict",
      "path": "./dict.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    // Binaries
    "**/wasm/**",
    "**/*.a",
    "**/*.dll",
    "**/*.dylib",
    "**/*.jar",
    "**/*.node",
    "**/*.so",
    "**/*.wasm",
    "**/dist/**",
    "**/lib/ios/**/*",
    "**/*.svg",

    // iOS
    "**/*.pbxproj",
    "**/*.plist",
    "**/*.storyboard",
    "**/*.xcconfig",
    "**/*.xcscheme",
    "**/contents.xcworkspacedata",

    // Java
    "**/build.gradle",
    "**/gradle-wrapper.properties",
    "**/gradle.properties",
    "**/gradlew",
    "**/gradlew.bat",
    "**/proguard-rules.pro",

    // Python
    "**/__pycache__/**",

    // JS
    "**/lib/wasm/**/*.js"
  ]
}